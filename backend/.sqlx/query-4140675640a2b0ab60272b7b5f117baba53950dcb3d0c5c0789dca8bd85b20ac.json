  "query": "    UPDATE users\nSET \n    username = COALESCE($1, username),\n    display_name = COALESCE($2, display_name),\n    bio = COALESCE($3, bio),\n    icon_url = COALESCE($4, icon_url),\n    updated_at = NOW()\nWHERE id = $5\nRETURNING\n    id,\n    username,\n    email,\n    password,\n    display_name,\n    bio,\n    icon_url,\n    is_official,\n    created_at,\n    updated_at;",
